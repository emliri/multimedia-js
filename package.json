{
  "name": "multimedia",
  "version": "1.2.0",
  "description": "Multimedia streaming framework for browser and node",
  "main": "index.ts",
  "scripts": {
    "build": "webpack --progress",
    "build-decls": "tsc --declaration --emitDeclarationOnly --outDir dist/",
    "build-decls-post": "mv dist/index.d.ts dist/mmjs.umd.d.ts",
    "build-watch": "DEBUG=1 npm run build -- --watch",
    "lint": "eslint index.ts src test-cases --ext .ts --ext .js",
    "lint-fix": "npm run lint -- --fix",
    "prepublishOnly": "npm test && make",
    "start": "DEBUG=1 webpack serve --progress",
    "test": "jest"
  },
  "keywords": [],
  "repository": "github.com/tchakabam/multimedia.js.git",
  "author": "Stephan Hesse <stephan@emliri.com>, <stephan_hesse@yahoo.com>",
  "license": "LGPL, Copyright (c) 2016, 2017, 2018, 2019, 2020 Stephan Hesse, EMLIRI Consultancy Berlin",
  "public": true,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "vendor/ffmpeg.js"
  ],
  "dependencies": {
    "bootstrap": "^4.1.3",
    "bowser": "^1.9.4",
    "eventemitter3": "^4.0.0",
    "ffmpeg.js": "^3.1.9001",
    "file-saver": "git+https://tchakabam@github.com/eligrey/FileSaver.js.git#4a73fd7a2dcdc6ce2947433af3d3fbad56a019fa",
    "m3u8-parser": "^4.2.0",
    "node-http-xhr": "^1.3.4",
    "url-toolkit": "^2.1.4",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.0",
    "@types/jest": "^21.1.5",
    "@types/node": "^10.12.0",
    "@types/webpack": "^4.4.17",
    "eslint": "^6.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^26.6.3",
    "should": "^13.1.3",
    "ts-jest": "^26.5.5",
    "ts-loader": "^8.0.7",
    "ts-node": "^4.1.0",
    "typescript": "^3.8.3",
    "typescript-eslint-parser": "^22.0.0",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0",
    "worker-loader": "^3.0.5"
  }
}
